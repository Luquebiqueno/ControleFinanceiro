{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=localhost;Initial Catalog=ControleFinanceiro;Integrated Security=True; TrustServerCertificate=True;"
  },
  "TokenConfiguration": {
    "Audience": "ControleFinanceiroAudience",
    "Issuer": "ControleFinanceiroIssuer",
    "Secret": "ControleFinanceiro_Secret",
    "Minutes": 60,
    "DaysToExpiry": 7
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
